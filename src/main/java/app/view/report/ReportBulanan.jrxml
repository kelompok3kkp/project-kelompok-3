	<queryString language="SQL">
		<![CDATA[SELECT 
    DATE_FORMAT(n.tgl_nota, '%Y-%m') AS bulan,
    COUNT(DISTINCT n.id_nota) AS jumlah_transaksi,
    SUM(i.harga) AS total_harga
FROM 
    nota n
JOIN 
    isi i ON n.id_nota = i.id_nota
GROUP BY bulan
ORDER BY bulan;]]>
	</queryString>

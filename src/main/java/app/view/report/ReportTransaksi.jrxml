	<queryString language="SQL">
		<![CDATA[SELECT
     nota.`id_nota` AS nota_id_nota,
     nota.`tgl_nota` AS nota_tgl_nota,
     nota.`id_pelanggan` AS nota_id_pelanggan,
     nota.`id_karyawan` AS nota_id_karyawan,
     isi.`id_nota` AS isi_id_nota,
     isi.`id_layanan` AS isi_id_layanan,
     isi.`jenis_layanan` AS isi_jenis_layanan,
     isi.`model_kendaraan` AS isi_model_kendaraan,
     isi.`harga` AS isi_harga

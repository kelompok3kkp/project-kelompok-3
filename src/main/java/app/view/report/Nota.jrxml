  <!-- PARAMETER harus didefinisikan sebelum queryString dan field -->
  <parameter name="id_pembayaran" class="java.lang.String"/>

  <queryString language="SQL"><![CDATA[
SELECT
  p.id_pembayaran                     AS pembayaran_id_pembayaran,
  p.tanggal_transaksi                 AS pembayaran_tanggal_transaksi,
  p.id_transaksi                      AS pembayaran_id_transaksi,
  p.harga                              AS pembayaran_harga,
  p.diskon                             AS pembayaran_diskon,
  p.metode_pembayaran                  AS pembayaran_metode_pembayaran,
  p.total_bayar                        AS pembayaran_total_bayar,
  t.id_pelanggan                       AS transaksi_id_pelanggan,
  t.id_karyawan                        AS transaksi_id_karyawan
FROM pembayaran p
LEFT JOIN transaksi t ON t.id_transaksi = p.id_transaksi
WHERE p.id_pembayaran = $P{id_pembayaran}
]]></queryString>

